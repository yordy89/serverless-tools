{
  "name": "serverless-tools",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-coverage-unit": "jest --coveragePathIgnorePatterns=mock --coverage --silent --env=node",
    "build": "webpack-cli",
    "watch": "webpack-cli -w",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "sam local start-api --env-vars env.json",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@types/aws-lambda": "^8.10.108",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.6",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "aws-sam-webpack-plugin": "^0.12.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "form-data": "^4.0.0",
    "puppeteer-core": "^19.2.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      ["@semantic-release/commit-analyzer",{
        "preset": "angular",
        "releaseRules": [
          {"type": "feat", "release": "minor"},
          {"type": "fix", "scope": "test", "release": "patch"},
          {"type": "perf", "release": "patch"},
          {"type": "refactor", "release": "patch"},
          {"type": "style", "release": "patch"},
          {"type": "test", "release": "patch"},
          {"type": "chore", "release": "patch"},
          {"type": "docs", "release": "patch"},
          {"type": "build", "release": "patch"},
          {"type": "ci", "release": "patch"},
          {"type": "revert", "release": "patch"},
          {"type": "branking", "release": "major"}
        ]
      }],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      ["@semantic-release/npm", {
        "npmPublish": false
      }],
      "@semantic-release/github"
    ]
  }
}
